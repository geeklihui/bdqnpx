<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE struts PUBLIC
	"-//Apache Software Foundation//DTD Struts Configuration 2.3//EN"
	"http://struts.apache.org/dtds/struts-2.3.dtd">

<struts>
	<constant name="struts.enable.DynamicMethodInvocation"
		value="true" />

	<package name="default" namespace="/" extends="struts-default">

		<action name="user_*" method="{1}"
			class="com.bdqno.web.action.UserAction">
			<result  name="login">/index.jsp</result>
			<result name="register">/login.jsp</result>
			<result name="findpwd">/login.jsp</result>
			<result name="fail">/error.jsp</result>
		</action>
			
		<action name="goods_*" method="{1}"
			class="com.bdqno.web.action.GoodsAction">
			<result name="1">/goods/goods1.jsp</result>
			<result name="2">/goods/goods2.jsp</result>
			<result name="3">/goods/goods3.jsp</result>
			<result name="4">/goods/goods4.jsp</result>
			<result name="5">/goods/goods5.jsp</result>
			<result name="6">/goods/goods6.jsp</result>
			<result name="7">/goods/goods7.jsp</result>
			<result name="8">/goods/goods8.jsp</result>
			<result name="9">/goods/goods9.jsp</result>
			<result name="10">/goods/goods10.jsp</result>
			<result name="11">/goods/goods11.jsp</result>
			<result name="12">/goods/goods12.jsp</result>
			<result name="all">/index.jsp</result>
			<result name="activity">/list.jsp</result>
			<result name="fail">/error.jsp</result>
		</action>

	</package>
	

	<!-- Add packages here -->

</struts>
